{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "target": "ESNext",
    "moduleResolution": "Node",
    "jsx": "preserve",
    "esModuleInterop": true,
    "strictPropertyInitialization": true,
    "strictNullChecks": true,
    "typeRoots": [
      "./node_modules/@types",
      "./app/js/@types",
      "./@types",
    ]
  },
  "exclude": [
    "node_modules",
  ]
}
