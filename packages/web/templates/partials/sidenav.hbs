{{#*inline "albums"}}
  <ui-iconlist>
    {{#each this.albums}}
      <ui-iconlistitem icon="images" label="{{this.name}}">
        {{#if (gt (len this.children) 0)}}
          {{> albums albums=this.children}}
        {{/if}}
      </ui-iconlistitem>
    {{/each}}
  </ui-iconlist>
{{/inline}}

<ui-iconlist>
  {{#each catalogs}}
    <ui-iconlistitem icon="file-earmark-richtext" label="{{this.name}}">
      <ui-iconlist>
        {{#if (gt (len this.searches) 0)}}
          <ui-iconlistitem icon="search" label="Saved Searches">
            <ui-iconlist>
              {{#each this.searches}}
                <ui-iconlistitem icon="search" label="{{this.name}}"></ui-iconlistitem>
              {{/each}}
            </ui-iconlist>
          </ui-iconlistitem>
        {{/if}}
        {{#if (gt (len this.albums) 0)}}
          <ui-iconlistitem icon="images" label="Albums">
            {{> albums albums=this.albums}}
          </ui-iconlistitem>
        {{/if}}
      </ui-iconlist>
    </ui-iconlistitem>
  {{/each}}
</ui-iconlist>
