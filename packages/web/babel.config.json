{
  "targets": "defaults",
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": "3.6.5"
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      { "version": "2018-09", "decoratorsBeforeExport": true }
    ],
    ["@babel/plugin-transform-class-properties"]
  ]
}
