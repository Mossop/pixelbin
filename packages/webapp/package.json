{
  "name": "webapp",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:remix": "remix vite:build",
    "build:css": "npm run css -- --style=compressed",
    "dev:remix": "node --import ./instrumentation.mjs node_modules/.bin/remix vite:dev",
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run css -- --watch",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "css": "sass -I styles styles/main.scss public/styles.css",
    "start": "node --import ./instrumentation.mjs node_modules/.bin/remix-serve ./build/server/index.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "@mdi/react": "^1.6.1",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/core": "^1.21.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.48.0",
    "@opentelemetry/instrumentation-express": "^0.35.0",
    "@opentelemetry/sdk-metrics": "^1.21.0",
    "@opentelemetry/sdk-node": "^0.48.0",
    "@opentelemetry/sdk-trace-node": "^1.21.0",
    "@remix-run/node": "^2.7.1",
    "@remix-run/react": "^2.7.1",
    "@remix-run/serve": "^2.7.1",
    "@types/luxon": "^3.4.2",
    "clsx": "^2.1.0",
    "immer": "^10.0.3",
    "isbot": "^4.1.0",
    "luxon": "^3.4.4",
    "md5": "^2.3.0",
    "mime-types": "^2.1.35",
    "ndjson-stream": "^1.0.0",
    "opentelemetry-instrumentation-remix": "^0.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@mossop/config": "^1.3.3",
    "@remix-run/dev": "^2.7.1",
    "@types/chromecast-caf-sender": "^1.0.9",
    "@types/md5": "^2.3.5",
    "@types/mime-types": "^2.1.4",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
